{
  "name": "match3-express",
  "version": "1.0.0",
  "description": "Express Backend for match 3",
  "main": "app.js",
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "author": "Francisco Rafart",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4",
    "immutable": "^4.0.0-rc.12",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.5",
    "redux": "^4.0.1",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "jest": "^24.0.0",
    "nodemon": "^1.18.6"
  }
}
